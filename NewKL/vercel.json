{
    "version":2,
    
    "builds":[
        {
        "src":"NewKL/wsgi.py",
        "use":"@vercel/python",
        "config":{
            "maxLambdaSize":"15mb","runtime":"python3.9"
        }
    },
    {
        "src":"build_files.sh",
        "use":"@vercel/static-build",
        "config":{"distDir":"staticfiles_build"}
    }
],
    "route":[
        
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"NewKL/wsgi.py"
        }
    ]
}